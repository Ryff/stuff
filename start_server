/home/builder/astonia3_server/server -a 34 -d
sleep 1
/home/builder/astonia3_server/server -a 35 -d
sleep 1
/home/builder/astonia3_server/server -a 36 -d
sleep 1
/home/builder/astonia3_server/server -a 37 -d
sleep 1
echo 'Astonia 3 Server Zones Loaded'
