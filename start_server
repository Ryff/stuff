#!/bin/sh

cd /home/builder/astonia3_server

killall looping

echo "Starting the Chatserver"
/home/builder/astonia3_server/chatserver >/dev/null & 
sleep1
/home/builder/astonia3_server/server -a 1 -d
sleep 1
/home/builder/astonia3_server/server -a 2 -d
sleep 1
/home/builder/astonia3_server/server -a 3 -d
sleep 1
