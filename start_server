sleep 1
/home/builder/astonia3_server/server -a 7 -d
sleep 1
/home/builder/astonia3_server/server -a 8 -d
sleep 1
/home/builder/astonia3_server/server -a 9 -d
sleep 1
/home/builder/astonia3_server/server -a 10 -d
sleep 1
/home/builder/astonia3_server/server -a 11 -d
sleep 1
/home/builder/astonia3_server/server -a 12 -d
sleep 1
/home/builder/astonia3_server/server -a 13 -d
sleep 1
/home/builder/astonia3_server/server -a 14 -d
sleep 1
/home/builder/astonia3_server/server -a 15 -d
sleep 1
/home/builder/astonia3_server/server -a 16 -d
sleep 1
/home/builder/astonia3_server/server -a 17 -d
sleep 1
/home/builder/astonia3_server/server -a 18 -d
sleep 1
/home/builder/astonia3_server/server -a 19 -d
sleep 1
/home/builder/astonia3_server/server -a 20 -d
sleep 1
/home/builder/astonia3_server/server -a 21 -d
sleep 1
/home/builder/astonia3_server/server -a 22 -d
sleep 1
/home/builder/astonia3_server/server -a 23 -d
sleep 1
