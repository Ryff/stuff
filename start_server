sleep 1
/home/builder/astonia3_server/looping & # a 27
sleep1
/home/builder/astonia3_server/server -a 28 -d
sleep 1
/home/builder/astonia3_server/server -a 29 -d
sleep 1
/home/builder/astonia3_server/server -a 30 -d
sleep 1
/home/builder/astonia3_server/server -a 31 -d
